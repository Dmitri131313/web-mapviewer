{
    "name": "web-mapviewer",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "npm run dev",
        "dev": "vite --port 8080",
        "preview": "vite build && vite preview --port 8080",
        "test:unit": "vitest --run --environment jsdom",
        "test:e2e": "start-server-and-test preview 8080 'cypress open'",
        "test:e2e:tablet": "start-server-and-test preview 8080 'cypress open --config viewportWidth=768,viewportHeight=1024'",
        "test:e2e:desktop": "start-server-and-test preview 8080 'cypress open --config viewportWidth=1440,viewportHeight=900'",
        "test:e2e:ci": "start-server-and-test preview 8080 'cypress run'",
        "test:ci": "npm run test:unit && npm run test:e2e:ci",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "build": "npm run type-check && vite build",
        "deploy:dev": "DEPLOY=true npm run build -- --mode development && node scripts/deploy.js dev",
        "deploy:int": "DEPLOY=true npm run build -- --mode development && node scripts/deploy.js int",
        "update:translations": "node scripts/generate-i18n-files.js",
        "update:browserlist": "npx browserslist@latest --update-db"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.1.2",
        "@fortawesome/free-brands-svg-icons": "^6.1.2",
        "@fortawesome/free-regular-svg-icons": "^6.1.2",
        "@fortawesome/free-solid-svg-icons": "^6.1.2",
        "@fortawesome/vue-fontawesome": "^3.0.0-5",
        "@geoblocks/ol-maplibre-layer": "^0.0.4",
        "@ivanv/vue-collapse-transition": "^1.0.2",
        "@popperjs/core": "^2.11.6",
        "animate.css": "^4.1.1",
        "axios": "^0.27.2",
        "bootstrap": "^5.2.0",
        "d3": "^7.6.1",
        "file-saver": "^2.0.5",
        "form-data": "^4.0.0",
        "hammerjs": "^2.0.8",
        "jquery": "^3.6.0",
        "maplibre-gl": "^2.3.0",
        "ol": "^7.0.0",
        "pako": "^2.0.4",
        "print-js": "^1.6.0",
        "proj4": "^2.8.0",
        "register-service-worker": "^1.7.2",
        "reproject": "^1.2.6",
        "tippy.js": "^6.3.7",
        "vue": "^3.2.37",
        "vue-i18n": "^9.2.2",
        "vue-router": "^4.1.3",
        "vue-social-sharing": "next",
        "vuex": "^4.0.2"
    },
    "devDependencies": {
        "@cypress/vite-dev-server": "^3.1.1",
        "@cypress/vue": "^4.1.0",
        "@nuintun/qrcode": "^3.3.0",
        "@rushstack/eslint-patch": "^1.1.4",
        "@types/jsdom": "^20.0.0",
        "@types/node": "^16.11.47",
        "@vitejs/plugin-vue": "^3.0.3",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^11.0.0",
        "@vue/tsconfig": "^0.1.3",
        "aws-sdk": "^2.1195.0",
        "caniuse-lite": "^1.0.30001377",
        "chai": "^4.3.6",
        "cypress": "^10.5.0",
        "cypress-browser-permissions": "^1.1.0",
        "cypress-recurse": "^1.21.0",
        "cypress-vite": "^1.1.1",
        "cypress-wait-until": "^1.7.2",
        "dotenv": "^16.0.1",
        "eslint": "^8.22.0",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-prettier-vue": "^4.2.0",
        "eslint-plugin-vue": "^9.3.0",
        "git-branch": "^2.0.1",
        "googleapis": "^105.0.0",
        "jsdom": "^20.0.0",
        "mime-types": "^2.1.35",
        "prettier": "^2.7.1",
        "prettier-plugin-jsdoc": "^0.3.38",
        "sass": "^1.54.4",
        "start-server-and-test": "^1.14.0",
        "typescript": "~4.7.4",
        "vite": "^3.0.8",
        "vite-plugin-require": "^1.0.1",
        "vitest": "^0.22.0",
        "vue-loader": "^17.0.0",
        "vue-tsc": "^0.40.1",
        "yargs": "^17.5.1"
    },
    "engines": {
        "node": ">=14",
        "npm": "8.*.*"
    },
    "type": "commonjs",
    "engineStrict": true
}
