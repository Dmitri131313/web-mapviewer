{
  "name": "web-mapviewer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port=${PORT:-$npm_package_config_port}",
    "test:unit": "cypress run",
    "lint": "vue-cli-service lint",
    "build:dev": "vue-cli-service build --mode=development",
    "build:prod": "vue-cli-service build",
    "cypress:open": "cypress open --config baseUrl=http://localhost:${PORT:-$npm_package_config_port}",
    "cypress:run": "cypress run --config baseUrl=http://localhost:${PORT:-$npm_package_config_port} --env video=false",
    "deploy:dev": "DEPLOY=true npm run build:dev && node scripts/deploy.js dev",
    "deploy:int": "DEPLOY=true npm run build:prod && node scripts/deploy.js int",
    "test:headless": "start-server-and-test serve http://localhost:${PORT:-$npm_package_config_port} cypress:run",
    "update:translations": "node scripts/generate-i18n-files.js"
  },
  "config": {
    "port": 8080
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@turf/turf": "^5.1.6",
    "@yzfe/svgicon": "^1.0.1",
    "@yzfe/vue-svgicon": "^4.0.1",
    "axios": "^0.20.0",
    "bootstrap": "^4.5.2",
    "core-js": "^3.6.5",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.1",
    "ol": "^6.4.3",
    "popper.js": "^1.16.1",
    "proj4": "^2.6.1",
    "register-service-worker": "^1.7.1",
    "reproject": "^1.2.6",
    "vue": "^2.6.12",
    "vue-i18n": "^8.22.0",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.7",
    "@vue/cli-plugin-eslint": "~4.5.7",
    "@vue/cli-plugin-pwa": "~4.5.7",
    "@vue/cli-plugin-unit-jest": "~4.5.7",
    "@vue/cli-plugin-vuex": "~4.5.7",
    "@vue/cli-service": "~4.5.7",
    "@vue/test-utils": "1.1.0",
    "@yzfe/svgicon-loader": "^1.0.2",
    "@yzfe/vue-cli-plugin-svgicon": "~1.0.1",
    "aws-sdk": "^2.771.0",
    "babel-eslint": "^10.1.0",
    "cypress": "^5.3.0",
    "eslint": "^6.8.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-vue": "^6.2.2",
    "git-branch": "^2.0.1",
    "googleapis": "^59.0.0",
    "mime-types": "^2.1.27",
    "sass": "^1.27.0",
    "sass-loader": "^8.0.2",
    "start-server-and-test": "^1.11.5",
    "vue-template-compiler": "^2.6.12",
    "yargs": "^15.4.1"
  }
}
